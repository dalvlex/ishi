# variable format .varname=varcontent

# web daemon: either nginx or apache2
.web=nginx

# PHP supported version: 7.0, 7.1, 7.2
.php=7.2

# how much time to keep backups, depending on type
.keep_weekly=4
.keep_daily=6
.keep_user=5

# paths to configuration files
.lock_sites=/root/ishi/etc/sites-locked

# path to backup storage folder
.backup_path=/root/ishi/var/backups

# SSH keys
.ssh=vlad=ssh-rsa  vlad.protopopescu@gmail.com
